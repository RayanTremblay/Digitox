{
  "expo": {
    "name": "Detoxly",
    "slug": "Detoxly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1D2024"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.detoxly.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1D2024"
      },
      "package": "com.detoxly.app",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7e55e60d-9264-49da-94a3-ad8d46f59d03"
      }
    },
    "plugins": [
      "expo-font",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo/vector-icons/build/vendor/react-native-vector-icons/Fonts/Ionicons.ttf"
          ]
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "iosAppId": "ca-app-pub-3131985902128037~3498329011",
          "androidAppId": "ca-app-pub-3131985902128037~3498329011"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#6E7A8A",
          "sounds": [
            "./assets/notification-sound.wav"
          ]
        }
      ]
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#6E7A8A"
    },
    "owner": "rayantremblay"
  }
}
